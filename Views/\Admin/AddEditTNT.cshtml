@model ApprovalPortal.Models.Masters

@{
    ViewBag.Title = "AddEditFiscalYear";
    Layout = "~/Views/Shared/AdminLTE/_Layout.cshtml";
}

<h2>Manage Tag/Nature of Transaction</h2>

@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()

    <div class="form-horizontal">
        <hr />
        @Html.ValidationSummary(true)
        <div class="form-group">
            @Html.Label("Tag/Nature of Transaction", new { @class = "control-label col-md-2" })
            <div class="col-md-10">
                @Html.HiddenFor(model => model.ID)
                @Html.EditorFor(model => model.Text)
                @Html.ValidationMessageFor(model => model.Text)
            </div>
        </div>

        <div class="form-group">
            @Html.Label("Active", new { @class = "control-label col-md-2" })
            <div class="col-md-10">
                @Html.EditorFor(model => model.IsActive)
                @Html.ValidationMessageFor(model => model.IsActive)
            </div>
        </div>




        <div class="form-group">
            <div class="col-md-offset-2 col-md-10">
                <input type="submit" value="Submit" class="btn btn-primary1" />
            </div>
        </div>
    </div>
}

<div>
    @Html.ActionLink("Back to List", "IndexTNT")
</div>
